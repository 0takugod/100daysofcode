class Solution {
    public int[] twoSum(int[] nums, int target){
        if(nums.length<3){
            return new int[]{0,1};
        }
        int m=0;
        for(int i=0;i<nums.length;i++){
            m=target-nums[i];
            for(int j=0;j<nums.length;j++){
                if(nums[j]==m && i!=j){
                    return new int[]{i,j};
                }
            }
            
        }
        return new int[]{-1};
    }
}
