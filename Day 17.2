class Solution {
    public List<Integer> minSubsequence(int[] nums) {
    
    Arrays.sort(nums);
    List<Integer> a=new ArrayList<Integer>();
    int s=0;
    for(int i=0;i<nums.length;i++)
    {
        s+=nums[i];
    }
    int l=0;
    for(int i=nums.length-1;i>-1;i--)
    {
        if(nums[i]+l>s-nums[i])
        {
            a.add(nums[i]);
            break;
        }
        else
        {a.add(nums[i]);
        l+=nums[i];
        s-=nums[i];}
    }
    return a;
    }
}
