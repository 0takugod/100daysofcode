class Solution {
    public int findNumbers(int[] nums) {
        int m=0;
        for(int i=0;i<nums.length;i++){
            int t=nums[i];
            int res=0;
            while(t>0){
                res++;
                t=t/10;
            }
            if(res%2==0){
                m++;
            }
        }
        return m;
    }
}
