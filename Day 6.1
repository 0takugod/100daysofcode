class Solution {
    public List<List<Integer>> largeGroupPositions(String s) {
        List<List<Integer>> res = new ArrayList<>();
        int i = 0;
          for (int j = 1; j < s.length(); j++) {
              while (j < s.length() && s.charAt(i) == s.charAt(j)) {
                  j++;
              }
              if (j - i >= 3) {
                 res.add(Arrays.asList(i, j - 1));
             }
             i = j;
         }
         return res;
        
    }
}
