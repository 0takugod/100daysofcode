class Solution {
    public int[] buildArray(int[] nums) {
        int a[]=new int[nums.length];
        int k=0;
        for(int i=0;i<nums.length;i++){
            for(int j=0;j<=i;j++){
                if (nums[nums[i]]==a[j])
                    break;
                if(j==i){
                    a[i]=nums[nums[i]];
                    
                }
                
                    
            }
        }
        return a;
        
    }
}
