import java.util.*;  
class cricket  
{  
  public static void main(String[] args) throws java.lang.Exception 
  {  
    Scanner sc= new Scanner(System.in);
    int a= sc.nextInt();
    int i,j;
    int ind = 0;
    int draw = 0;
    int eng = 0;
    int arr[][] = new int[a][5];
    	for(i=0;i<a;i++){
    		for(j=0;j<5;j++){
    			arr[i][j] = sc.nextInt();
    		}
    	}
    	for(i=0;i<a;i++){
    		for(j=0;j<5;j++){
    			if(arr[i][j]==0){ draw = draw+1;}
    			else if(arr[i][j]==1){ ind = ind+1;}
    			else{
    				eng = eng+1;
    			}
    		}
    		if((draw>ind && draw>eng)||(eng==ind)){ System.out.println("DRAW"); }
    		else if(ind>=draw && ind>eng){ System.out.println("INDIA");}
    		else{System.out.println("ENGLAND");}
    		draw = 0;
    		ind = 0;
    		eng = 0;
    	}
    }
} 
