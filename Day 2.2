class Solution {
    public int repeatedNTimes(int[] nums) {
        int n = nums.length;
        int i;
        int j;
        int m = 0;
        for(i=0;i<n;i++){
            for(j=0;j<n;j++){
                if(nums[i]==nums[j]){
                    m = m + 1;
                }
            }
            if(m > 1){
                int p = nums[i];
                return p;
            }
            m = 0;
            
        }
     return 0;
    }
}
